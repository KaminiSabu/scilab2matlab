
function [n,d]=iirnotch(w0, bw)
//w0=1/3
//bw=1/12

bw1=%pi*bw/2;       //notch frequency to cutoff frequency
w=%pi*w0            //notch frequency
k2=(1-tan(bw1))/(1+tan(bw1));   //
// numerator coefficients
n(1)=(1+k2)/2;
n(2)=-2*n(1)*cos(w);
n(3)=n(1);
// denominator coefficients
d(1)=1;
d(2)=-(1+k2)*cos(w);
d(3)=k2;

endfunction
